# Yahoo Finance MCP Tool - Environment Variables

# Trading simulator API endpoint
TRADING_SIMULATOR_ENDPOINT=https://your-trading-simulator.com/api/stock-data

# API key for the trading simulator (if required)
TRADING_SIMULATOR_API_KEY=your_api_key_here

# Default stock symbol
DEFAULT_SYMBOL=AAPL

# Browser settings
HEADLESS=true
BROWSER_WIDTH=1920
BROWSER_HEIGHT=1080
BROWSER_TIMEOUT=30000

# Logging
LOG_LEVEL=info